+/**
+ * Tests for the BadEncodingParseFilter
+ *
+ * @author markus@openindex.io
+ */
+public class TestBadEncodingParseFilter extends TestCase {
+  private final static Logger LOG = LoggerFactory.getLogger(TestBadEncodingParseFilter.class);
+  private final static String SEPARATOR = System.getProperty("file.separator");
+  private final static String SAMPLES = System.getProperty("test.models", ".");
+  
+  private Configuration conf;
+
+  private BadEncodingParseFilter filter = new BadEncodingParseFilter();
+
+  public TestBadEncodingParseFilter() {
+    conf = NutchConfiguration.create();
+    conf.set("plugin.includes", "parse-tika");
+    filter.setConf(conf);
+  }
+  
+  public void testBadURLs() throws Exception {
+    assertTrue(getPage("bad-urls.html"));
+    assertFalse(getPage("no-bad-urls.html"));
+  }
+  
+  private boolean getPage(String uri) throws Exception {
+    String url = "http://example.org/" + uri;
+    Reader reader = conf.getConfResourceAsReader("badencoding-detector/test/test-documents/" + uri);
+    StringBuilder html = new StringBuilder();
+    
+    int numBadUrls = 0;
+    
+    try {
+      BufferedReader bufferedReader = new BufferedReader(reader);
+      String line = null;
+      while ((line = bufferedReader.readLine()) != null) {
+        html.append(line);
+      }
+    } catch ( IOException e) {
+      e.printStackTrace();
+      return false;
+    }
+    
+    Content content = new Content(url, url, html.toString().getBytes(), "text/html", new Metadata(), conf);
+    
+    try{
+      ParseResult parseResult = new ParseUtil(conf).parse(content);
+      ParseData parseData = parseResult.get(url).getData();
+      
+      numBadUrls = filter.findBadURLs(parseData.getOutlinks());
+    } catch (Exception e) {
+      LOG.error("Error parsing: StringUtils.stringifyException(e));
+    }
+
+    return numBadUrls > 0 ? true : false;
+  }
