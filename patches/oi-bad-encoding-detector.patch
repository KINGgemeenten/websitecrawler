+package io.openindex.nutch.parse.badencoding;
+
+import org.apache.hadoop.io.Text;
+import org.apache.nutch.metadata.Metadata;
+import org.apache.nutch.parse.HTMLMetaTags;
+import org.apache.nutch.parse.HtmlParseFilter;
+import org.apache.nutch.parse.Outlink;
+import org.apache.nutch.parse.Parse;
+import org.apache.nutch.parse.ParseResult;
+import org.apache.nutch.parse.ParseUtil;
+import org.apache.nutch.protocol.Content;
+import org.apache.nutch.util.URLUtil;
+import org.apache.nutch.parse.ParseUtil;
+import org.apache.nutch.util.NutchConfiguration;
+
+import org.slf4j.Logger;
+import org.slf4j.LoggerFactory;
+import org.w3c.dom.DocumentFragment;
+
+import org.apache.hadoop.conf.Configuration;
+
+/**
+ * Bad URL Encoding Detector parse filter. Parser filter impl. to detect bad encoded URL's and
+ * mark this page as having bad URL's.
+ * 
+ * @author markus@openindex.io
+ */
+public class BadEncodingParseFilter implements HtmlParseFilter {
+  public static final Logger LOG = LoggerFactory.getLogger(BadEncodingParseFilter.class);
+  private Configuration conf;
+  
+  /**
+   * Whether to mark pages containing bad URL's with a value of TRUE or
+   * using the amount of bad URL's in the page.
+   */
+  private boolean markAsAmount = false;
+  
+  /**
+   * Whether to write a value also if no bad URL's are found.
+   */
+  private boolean alsoWriteIfNoneFound = false;
+  
+  /**
+   * The field name to write to.
+   */
+  private String field = "badencoding";
+  
+  /**
+   * Default constructor
+   */
+  public BadEncodingParseFilter() {
+    conf = NutchConfiguration.create();
+  }
+
+  /**
+   * Bad encoding parse filter
+   *
+   * @param Content
+   * @param ParseResult
+   * @param HTMLMetaTags
+   * @param DocumentFragment
+   * @return ParseResult
+   */
+  public ParseResult filter(Content content, ParseResult parseResult,
+    HTMLMetaTags metaTags, DocumentFragment doc) {
+    
+    // Get the url
+    String url = content.getUrl();
+    
+    // Parse object voor deze URL
+    Parse parse = parseResult.get(url);
+    
+    // Only handle HTML pages
+    if (!(content.getContentType().equals("application/xhtml+xml") || content.getContentType().equals("text/html"))) {
+      return parseResult;
+    }
+    
+    // Detect the stuff!
+    int numBadURLs = findBadURLs(parse.getData().getOutlinks());
+
+    // Write stuff?
+    if (numBadURLs > 0 || alsoWriteIfNoneFound) {
+      // Make a value!
+      String value = markAsAmount ? Integer.toString(numBadURLs) : (numBadURLs > 0 ? "true" : "false");
+      
+      // !
+      parse.getData().getParseMeta().set(field, value);
+    }
+    
+    // Ok!
+    return parseResult;
+  }
+  
+  /**
+   * Check the specific outlinks for inproper encoding.
+   *
+   * @param outlinks the outlinks to check
+   * @return int
+   */
+  public int findBadURLs(Outlink[] outlinks) {
+    // Out counter
+    int numBadURLs = 0;
+    
+    for (int i = 0; i < outlinks.length; i++) {
+      // Get the target
+      String target = outlinks[i].getToUrl();
+      
+      // Check for spaces
+      if (target.indexOf(" ") != -1) {
+        numBadURLs++;
+        continue;
+      }
+      
+      // Check for non- 7-bit ASCII chars
+      for (int j = 0; j < target.length(); j++) {
+        // Got one?
+        if (target.charAt(j) > 127) {
+          numBadURLs++;
+          continue;
+        }
+      }      
+    }
+    
+    // Euy!
+    return numBadURLs;
+  }
+  
+  /**
+   * @param Configuration
+   */
+  public void setConf(Configuration conf) {
+    this.conf = conf;
+
+    markAsAmount = conf.getBoolean("badencoding.mark.amount", false);
+    alsoWriteIfNoneFound = conf.getBoolean("badencoding.write.always", false);
+    field = conf.get("badencoding.field", "badencoding");
+  }
+
+  /**
+   * @return Configuration
+   */
+  public Configuration getConf() {
+    return this.conf;
+  }
+}
